{
  "compilerOptions": {
    "target": "esnext",
    "lib": ["esnext"],
    "allowUnreachableCode": true,
    "allowJs": true,
    "skipLibCheck": true,
    "allowSyntheticDefaultImports": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "moduleResolution": "node",
    "strict": true,
    "baseUrl": "src/",
    "outDir": "dist/",
    "declaration": true,
    "sourceMap": true,
    "jsx": "react-native"
  },
  "exclude": ["node_modules", "docs", "dist", "examples"]
}
